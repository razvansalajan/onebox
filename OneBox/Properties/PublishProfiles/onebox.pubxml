<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://onebox.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>onebox.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>onebox</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName>$onebox</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects>
        <ObjectGroup Name="oneboxdatabase" Order="1" Enabled="False" xmlns="">
          <Destination Path="Data Source=tcp:oneboxsqlserver.database.windows.net,1433;Initial Catalog=oneboxdatabase;User ID=razvan@oneboxsqlserver;Password=Admin2016!;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="Server=tcp:oneboxsqlserver.database.windows.net,1433;Database=oneboxdatabase;User ID=razvan@oneboxsqlserver;Password=Admin2016!;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;MultipleActiveResultSets=True" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=tcp:oneboxsqlserver.database.windows.net,1433;Initial Catalog=oneboxdatabase;User ID=razvan@oneboxsqlserver;Password=Admin2016!;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\oneboxdatabase_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="  Server=tcp:oneboxsqlserver.database.windows.net,1433;Database=oneboxdatabase;User ID=razvan@oneboxsqlserver;Password=Admin2016!;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;MultipleActiveResultSets=True;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)oneboxdatabase-Web.config Connection String">
      <ParameterValue>Server=tcp:oneboxsqlserver.database.windows.net,1433;Database=oneboxdatabase;User ID=razvan@oneboxsqlserver;Password=Admin2016!;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;MultipleActiveResultSets=True</ParameterValue>
