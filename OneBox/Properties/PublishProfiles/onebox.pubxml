        <ObjectGroup Name="oneboxdatabase" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:onebox-server.database.windows.net,1433;Initial Catalog=oneboxdatabase;User ID=onebox@onebox-server;Password=d1jKstra1994" Name="Data Source=tcp:onebox-server.database.windows.net,1433;Initial Catalog=oneboxdatabase;User ID=onebox@onebox-server;Password=d1jKstra1994;MultipleActiveResultSets=True;" />
          <Object Type="DbCodeFirst">
